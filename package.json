{
  "name": "expense-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:new": "npx supabase migration new",
    "db:push": "npx supabase db push",
    "db:pull": "npx supabase db pull",
    "db:seed": "npx supabase db seed --file supabase/seed.sql",
    "db:reset": "npx supabase db reset",
    "db:types": "npx supabase gen types typescript --project-id $DEV_PROJECT_REF --schema public > supabase/types/database.types.ts",
    "db:dump": "npx supabase db dump --file supabase/schema_dump.sql",
    "db:link": "npx supabase link --project-ref $DEV_PROJECT_REF",
    "db:status": "npx supabase status",
    "db:stop": "npx supabase stop",
    "db:start": "npx supabase start"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.57.2",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "supabase": "^2.39.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
